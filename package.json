{
  "name": "hotware-dashboard",
  "version": "1.0.7",
  "description": "Hotware Dashboard",
  "author": "Hotware",
  "main": "electron/main.js",
  "private": true,
  "repository": "https://github.com/darrengunawan15/testing",
  "scripts": {
    "dev": "next dev -p 3130",
    "dev:electron": "electron electron/main.js",
    "build": "next build && node scripts/post-build.js",
    "start": "next start",
    "lint": "next lint",
    "electron": "electron electron/main.js",
    "dist": "npm run build && npx electron-builder --win --publish always",
    "setup": "node scripts/setup.js"
  },
  "build": {
    "appId": "hotware.dashboard",
    "productName": "Hotware Dashboard",
    "directories": {
      "output": "dist",
      "buildResources": "assets"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": true,
      "perMachine": false,
      "deleteAppDataOnUninstall": false,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "Hotware Dashboard",
      "allowElevation": true,
      "runAfterFinish": true
    },
    "files": [
      "electron/**/*",
      ".next/standalone/**",
      "package.json"
    ],
    "extraMetadata": {
      "main": "electron/main.js"
    },
    "asar": {
      "smartUnpack": true
    },
    "asarUnpack": [
      ".next/standalone/**"
    ],
    "extraResources": [
      {
        "from": "node-bin",
        "to": "node-bin",
        "filter": [
          "**/*"
        ]
      }
    ],
    "publish": [
      {
        "provider": "github",
        "releaseType": "release",
        "owner": "darrengunawan15",
        "repo": "testing"
      }
    ]
  },
  "dependencies": {
    "@dnd-kit/utilities": "^3.2.2",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-radio-group": "^1.3.8",
    "bcryptjs": "^3.0.3",
    "dotenv": "^16.4.5",
    "electron-updater": "^6.7.3",
    "jspdf": "^4.2.0",
    "jspdf-autotable": "^5.0.7",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-google-maps/api": "^2.20.8",
    "@tanstack/react-query": "^5.90.11",
    "@types/bcryptjs": "^2.4.6",
    "@types/js-cookie": "^3.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^20",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.0.1",
    "baseline-browser-mapping": "^2.9.19",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^2.30.0",
    "electron": "^40.2.1",
    "electron-builder": "^26.7.0",
    "eslint": "^9.39.2",
    "eslint-config-next": "15.0.1",
    "framer-motion": "^10.16.4",
    "hls.js": "^1.6.15",
    "jose": "^6.1.3",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.3",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.294.0",
    "mysql2": "^3.15.3",
    "next": "^16.1.6",
    "postcss": "^8",
    "react": "^19.2.4",
    "react-day-picker": "^9.12.0",
    "react-dom": "^19.2.4",
    "react-grid-layout": "^2.2.0",
    "react-hook-form": "^7.67.0",
    "react-icons": "^5.5.0",
    "react-leaflet": "^5.0.0",
    "react-router-dom": "^6.18.0",
    "recharts": "^2.8.0",
    "socket.io-client": "^4.7.2",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5",
    "zod": "^4.1.13",
    "zustand": "^4.4.6"
  },
  "overrides": {
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  }
}
